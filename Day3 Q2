#include <stdio.h>

int main()
{
    int n, i, sum = 0, total, missing;
    int arr[100];

    printf("Enter value of n: ");
    scanf("%d", &n);

    printf("Enter %d elements:\n", n - 1);
    for(i = 0; i < n - 1; i++)
    {
        scanf("%d", &arr[i]);
        sum += arr[i];
    }

    total = n * (n + 1) / 2;
    missing = total - sum;

    printf("Missing element is %d\n", missing);

    return 0;
}
